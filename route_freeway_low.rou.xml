<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">

  <vType id="car_freeway" vClass="passenger" guiShape="passenger"
         accel="2.6" decel="4.5" length="4.5" minGap="2.5"
         maxSpeed="33.33" laneChangeModel="LC2013" sigma="0.5"/>

  <route id="r_L_R" edges="E#L-R"/>
  <route id="r_R_L" edges="E#R-L"/>

  <!-- LOW density preset: begin=0, end=75 s; period=10s (~360 veh/h per lane) -->
    <flow id="L0" type="car_freeway" route="r_L_R" begin="0" end="75" period="10" departLane="0" departPos="free" departSpeed="max"/>
  <flow id="L1" type="car_freeway" route="r_L_R" begin="0" end="75" period="10" departLane="1" departPos="free" departSpeed="max"/>
  <flow id="L2" type="car_freeway" route="r_L_R" begin="0" end="75" period="10" departLane="2" departPos="free" departSpeed="max"/>

  <flow id="R0" type="car_freeway" route="r_R_L" begin="0" end="75" period="10" departLane="0" departPos="free" departSpeed="max"/>
  <flow id="R1" type="car_freeway" route="r_R_L" begin="0" end="75" period="10" departLane="1" departPos="free" departSpeed="max"/>
  <flow id="R2" type="car_freeway" route="r_R_L" begin="0" end="75" period="10" departLane="2" departPos="free" departSpeed="max"/>
</routes>
